    "expo": "~54.0.29",
    "expo-constants": "~18.0.13",
    "expo-router": "6.0.19",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
